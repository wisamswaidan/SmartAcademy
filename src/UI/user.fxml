   <TextField fx:id="password_TextField" layoutX="770.0" layoutY="392.0" promptText="Password" />
   <TextField fx:id="username_TextField" layoutX="770.0" layoutY="356.0" promptText="Username" />
   <TextField fx:id="zipcode_TextField" layoutX="770.0" layoutY="322.0" promptText="Zipcode" />
   <TextField fx:id="address_TextField" layoutX="770.0" layoutY="285.0" promptText="Address" />
   <TextField fx:id="phone_TextField" layoutX="770.0" layoutY="251.0" promptText="Telephone" />
   <TextField fx:id="lastname_TextField" layoutX="770.0" layoutY="214.0" promptText="Lastname" />
   <TextField fx:id="firstname_TextField" layoutX="770.0" layoutY="180.0" promptText="Firstname" />
   <Button fx:id="addBut" layoutX="771.0" layoutY="470.0" mnemonicParsing="false" onAction="#addUser" prefHeight="17.0" prefWidth="149.0" text="Add" />
   <Text layoutX="719.0" layoutY="136.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Would you like to add a new EASVSMART user?">
         <effect>
            <Lighting>
               <bumpInput>
                  <Shadow />
               </bumpInput>
               <light>
                  <Light.Distant />
               </light>
            </Lighting>
         </effect></Text>
      <Button fx:id="selectBut" layoutX="771.0" layoutY="511.0" mnemonicParsing="false" onAction="#selectToEditUser" prefHeight="25.0" prefWidth="149.0" text="Select" />
</children>
</AnchorPane>
