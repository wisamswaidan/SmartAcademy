<AnchorPane prefHeight="800.0" prefWidth="1050.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Application.UserController">
<children>
      <ImageView fitHeight="800.0" fitWidth="1050.0" layoutX="-1.0" layoutY="-1.0">
         <image>
            <Image url="@Images/user.jpg" />
         </image>
      </ImageView>
   <TableView fx:id="tableUserView" layoutX="37.0" layoutY="123.0" prefHeight="554.0" prefWidth="660.0">
      <columns>
         <TableColumn fx:id="col_firstname" text="Firstname" />
         <TableColumn fx:id="col_lastname" prefWidth="74.0" text="Lastname" />
         <TableColumn fx:id="col_phone" prefWidth="93.0" text="Phone" />
         <TableColumn fx:id="col_address" prefWidth="61.0" text="Address" />
         <TableColumn fx:id="col_zipcode" prefWidth="61.0" text="ZipCode" />
         <TableColumn fx:id="col_username" prefWidth="91.0" text="Username" />
         <TableColumn fx:id="col_password" prefWidth="93.0" text="Password" />
         <TableColumn fx:id="col_usertype" prefWidth="106.0" text="Usertype" />
      </columns>
   </TableView>
   <Button fx:id="editBut" layoutX="771.0" layoutY="553.0" mnemonicParsing="false" onAction="#editUser" prefHeight="25.0" prefWidth="149.0" text="Edit" />
   <Button fx:id="removeBut" layoutX="771.0" layoutY="595.0" mnemonicParsing="false" onAction="#removeUser" prefHeight="25.0" prefWidth="149.0" text="Delete" />
   <Button fx:id="backBut" layoutX="769.0" layoutY="637.0" mnemonicParsing="false" onAction="#backButton" prefHeight="25.0" prefWidth="149.0" text="Back" />
